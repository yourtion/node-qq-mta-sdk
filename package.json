{
  "name": "qq-mta",
  "version": "0.1.0",
  "description": "Node.js SDK for qq mta (Mobile Tencent Analytics)",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint ./lib ./test --fix",
    "test": "export DEBUG=mta:* && mocha -t 60000",
    "test-cov": "istanbul cover _mocha -- -t 60000",
    "test-coveralls": "npm run test-cov && cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run lint && npm run test-coveralls"
  },
  "keywords": [
    "MAT"
  ],
  "author": "Yourtion <yourtion@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.6.3",
    "request": "^2.81.0"
  },
  "devDependencies": {
    "coveralls": "^2.12.0",
    "eslint": "^3.18.0",
    "eslint-config-guo": "^0.3.0",
    "eslint-plugin-promise": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0"
  }
}
