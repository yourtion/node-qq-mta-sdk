{
  "name": "qq-mta",
  "version": "1.0.0",
  "description": "Node.js SDK for qq mta (Mobile Tencent Analytics)",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint ./lib ./test --fix",
